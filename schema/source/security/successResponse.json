{
  "$id": "successResponseSecurity",
  "type": "object",
  "required": [
    "headers",
    "body"
  ],
  "additionalProperties": true,
  "properties": {
    "headers": {
      "type": "object",
      "required": [
        "x-auth-token",
        "x-icbanking-session"
      ],
      "additionalProperties": true,
      "properties": {
        "x-auth-token": {
          "type": "string",
          "minLength": 3
        },
        "x-icbanking-session": {
          "type": "string",
          "minLength": 3
        }
      }
    },
    "body": {
      "type": "object",
      "required": [
        "user",
        "extendedProperties"
      ],
      "additionalProperties": true,
      "properties": {
        "user": {
          "type": "object",
          "required": [
            "documentNumber"
          ],
          "additionalProperties": true,
          "properties": {
            "documentNumber": {
              "type": "string",
              "minLength": 3
            }
          }
        },
        "extendedProperties": {
          "type": "object",
          "required": [
            "data"
          ],
          "additionalProperties": true,
          "properties": {
            "data": {
              "type": "array",
              "minItems": 1,
              "contains": {
                "type": "object",
                "required": [
                  "key",
                  "value"
                ],
                "additionalProperties": true,
                "properties": {
                  "key": {
                    "type": "string",
                    "enum": [
                      "RiskEvaluationCode"
                    ]
                  },
                  "value": {
                    "type": "object",
                    "required": [
                      "extendedPropertyValue"
                    ],
                    "additionalProperties": true,
                    "properties": {
                      "extendedPropertyValue": {
                        "type": "string",
                        "minLength": 3
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
